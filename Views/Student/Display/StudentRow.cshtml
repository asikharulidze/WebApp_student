@model Student

<tr>
    <td>@Model.Name</td>
    <td>@Model.PhoneNumber</td>
    <td>@Model.Email</td>
    <td>
        <a href="@Url.Action("Edit", "Student", new { id = Model.Id })" class="link-warning">
            Edit
        </a>
        <a href="@Url.Action("DeleteStudent", "Student", new { id = Model.Id })" class="link-danger">
            Delete
        </a>
    </td>
</tr>